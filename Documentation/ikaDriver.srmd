<?xml version="1.0" encoding="UTF-8"?>
<srmd:SimulationResourceMetaData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://apps.pmsf.net/STMD/SimulationResourceMetaData SRMD.xsd"
    xmlns:srmd="http://apps.pmsf.net/STMD/SimulationResourceMetaData"
    version="0.5" name="Sensor Model XXX Meta-Data">
    <srmd:Classification type="de.sl4to5.srmd.model-meta-data">
        <!-- Sensor related information -->
        <srmd:ClassificationEntry keyword="model.type">sensor</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.manufacturer">Company Name</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.family">Actual Sensor HW Family Name</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.name">Actual Sensor HW Name</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.version">Actual Sensor HW Version</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.manufacturer.partnumber">Partnumber at Manufacturer</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.type">Type of Output of Sensor, e.g. object-output</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.technology">HW Technology of sensor, e.g. radar</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="sensor.technology.variant">HW Technology variant of sensor, e.g. long-range</srmd:ClassificationEntry>
        <!-- Model related information -->
        <srmd:ClassificationEntry keyword="model.manufacturer">Company Name</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.family">Sensor Model Family Name</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.name">Sensor Model Name</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.version">Sensor Model Version</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.releasedate">Sensor Model Release Date, e.g. 2020-04-03</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.status">Development Status, e.g. In Development or Released, etc.</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.guid">For FMU-based models, this should be the GUID of the FMU, e.g. 7ae4-210f-dead-beef-0422</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.purpose">Purpose for which the model has been built/validated, e.g. HAD function validation in OEM environment at object-list level</srmd:ClassificationEntry>
        
        <srmd:ClassificationEntry keyword="model.modelling-approach">Description of the modelling approach taken, e.g. Sensor model is purely object-list driven, weather effects are not modelled.</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.preconditions">Preconditions for sensor model validity, e.g. Object-list input contains accurate bounding box values</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.validity-range">Restrictions on the sensor model validity, e.g. Sensor model is only accurate up to sensor/vehicle speeds of less than 150km/h</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.specification">Provide a link to the sensor model specification document</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.verification.status">Is the model verified, e.g. true or false</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.verification.report">Link to sensor model verification report, if any exists</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.validation.concept">Link to document detailing the validation concepts used for model validation, if any exists</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.validation.status">Is the model validated, according to the validation concept, e.g. true or false</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.validation.platform">Specification of simulation environment used for sensor model validation, e.g. PMSF FMIBench 1.9.9.4 using osi3test 0.6.0 on Windows 10 1809</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.validation.report">Link to sensor model validation report, if any exists</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.limitations">Description of all limitations that apply to the sensor model and its usage, e.g. This delivery is a pre-release and the model is intended to be used for checking the interfaces of the simulation toolchain.
            The model is not validated; thus it is not intended to be used for validation or verification of any sensor, especially not for ARS810 or ARS840. In particular the existence
            of this model shall not be used to imply the existence of sensors ARS810 nor ARS840 nor their properties.
            
            This delivery is not developed according to process defined in ISO 26262 (Standard for Functional Safety).
        </srmd:ClassificationEntry>
        
        <!-- Model implementation related information -->
        <srmd:ClassificationEntry keyword="model.implementation.format">Format of model implementation, e.g. OSMP 1.0/OSI 3.1.2/FMI 2.0</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.implementation.preconditions">Implementation requirements, e.g. Model needs Windows 10 1809 or newer with nVidia Drivers version 389.12 or newer and OpenCL 2.0 capable GPU</srmd:ClassificationEntry>
        <srmd:ClassificationEntry keyword="model.implementation.performance-characteristics">Description of model performance characteristics, e.g. Potentially real-time capable, if less than 100 objects in object list input at any one time</srmd:ClassificationEntry>
        
        <srmd:ClassificationEntry keyword="model.integration.requirements">Provide a link to requirements and considerations for operating the model</srmd:ClassificationEntry>
        
    </srmd:Classification>
</srmd:SimulationResourceMetaData>
